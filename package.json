{
  "name": "@shababul_alam/vue2-expand-on-tap",
  "version": "1.0.0",
  "description": "Expand section on tap build with Vue 2 âœ¨",
  "main": "index.js",
  "module": "dist/my-component.esm.js",
  "unpkg": "dist/my-component.min.js",
  "browser": {
    "./sfc": "src/my-component.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/my-component.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/my-component.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/my-component.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShababulAlam/vue2-expand-on-tap.git"
  },
  "keywords": [
    "collapsable",
    "vue2",
    "expand",
    "tap"
  ],
  "author": "@shababulalam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShababulAlam/vue2-expand-on-tap/issues"
  },
  "homepage": "https://github.com/ShababulAlam/vue2-expand-on-tap#readme",
  "devDependencies": {
    "rollup": "^1.17.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
